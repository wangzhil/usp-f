server:
  port:8886

spring:
  application:
    name:ribbon-client-a

eureka:
  instance:
    perferIpAddress:true
    instance-id:${spring.cloud.client.ipAddress}:${server.port}
  client:
    registerWithEureka:true
    fetchRegistry:true
    serviceUrl:defaultZone=http://${security.user.name}:${security.user.password}@localhost:8889/eureka/

security:
  user:
    name:admin
    password:123456

hystrix:
  command:
    default:
      execution:
        isolation:
          thread:
            timeoutInMilliseconds:5000