server:
  port:8886

spring:
  application:
    name:ribbon-client-a

security:
  user:
    name:admin
    password:123456

eureka:
  instance:
    perferIpAddress:true
    instance-id:${spring.cloud.client.ipAddress}:${server.port}
  client:
    registerWithEureka:true
    fetchRegistry:true
    
      defaultZone:http://admin:123456@localhost:8889/eureka/

hystrix:
  command:
    default:
      execution:
        isolation:
          thread:
            timeoutInMilliseconds:5000